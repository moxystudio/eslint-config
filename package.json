{
  "name": "eslint-config-moxy",
  "description": "MOXY eslint configuration to be used across several JavaScript projects",
  "version": "6.1.1",
  "keywords": [
    "eslint-config",
    "eslintconfig",
    "eslint",
    "lint",
    "linter",
    "style"
  ],
  "author": "Andr√© Cruz <andre@moxy.studio>",
  "homepage": "https://github.com/moxystudio/eslint-config",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/eslint-config.git"
  },
  "license": "MIT",
  "files": [
    "es*.js",
    "rules",
    "addons"
  ],
  "scripts": {
    "lint": "eslint . --ignore-pattern test/fixtures",
    "test": "jest --env node --coverage",
    "prerelease": "npm t && npm run lint",
    "release": "standard-version",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin master && npm publish"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-pattern test/fixtures",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "eslint": "^5.0.0"
  },
  "dependencies": {
    "babel-eslint": "8.2.2",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsdoc": "^3.7.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-prefer-import": "0.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "create-react-class": "^15.6.3",
    "eslint": "^5.3.0",
    "husky": "^0.14.0",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prop-types": "^15.6.2",
    "standard-version": "^5.0.0"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  }
}
