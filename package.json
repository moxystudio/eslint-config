{
  "name": "eslint-config-moxy",
  "description": "MOXY eslint configuration to be used across several JavaScript projects",
  "version": "2.1.0",
  "keywords": [
    "eslint-config",
    "eslintconfig",
    "eslint",
    "lint",
    "linter",
    "style"
  ],
  "author": "Andr√© Cruz <andre@moxy.studio>",
  "homepage": "https://github.com/moxystudio/eslint-config",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/eslint-config.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ignore-pattern test/fixtures",
    "lint:staged": "lint-staged",
    "test": "jest --env node --coverage",
    "test:watch": "jest --env node --watch",
    "posttest": "npm run lint",
    "precommit": "npm run lint:staged"
  },
  "lint-staged": {
    "*.js": "eslint --ignore-pattern test/fixtures"
  },
  "peerDependencies": {
    "eslint": "^3.0.0"
  },
  "dependencies": {
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-jsdoc": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^3.0.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.0"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  }
}
